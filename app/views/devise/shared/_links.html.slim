- if controller_name != 'sessions'
	= link_to t('users.login.title'), new_session_path(resource_name), class: 'btn btn-info btn-lg'

- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t('users.sing_up.title'), new_registration_path(resource_name), class: 'btn btn-info btn-lg'
